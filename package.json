{
  "name": "manifold-sdk",
  "version": "1.0.0",
  "description": "An SDK for the Manifold Markets API",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "Keri Warr <keri@warr.ca>",
  "license": "MIT",
  "packageManager": "yarn@3.2.2",
  "type": "module",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "esm"
    ]
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "lint": "yarn eslint --fix . && yarn prettier --write ."
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "2.7.1",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  }
}
