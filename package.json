{
  "name": "manifold-sdk",
  "version": "2.0.1",
  "description": "An SDK for the Manifold Markets API",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "author": "Keri Warr <keri@warr.ca>",
  "license": "MIT",
  "packageManager": "yarn@3.2.2",
  "type": "commonjs",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "lint": "yarn eslint --fix . && yarn prettier --write .",
    "test": "jest"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "fetch-intercept": "^2.4.0",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "node-fetch": "^2.6.7",
    "pinst": "^3.0.0",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4"
  },
  "homepage": "https://github.com/keriwarr/manifold-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/keriwarr/manifold-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/keriwarr/manifold-sdk/issues"
  },
  "keywords": [
    "TypeScript",
    "SDK"
  ]
}
